{
  "name": "capacitor-app",
  "version": "1.0.0",
  "description": "An Amazing Capacitor App",
  "keywords": [
    "capacitor",
    "mobile"
  ],
  "scripts": {
    "dev:turbo": "node ./scripts/printTurboMessage.js && NEXT_PUBLIC_TURBO_DEV=true next dev --turbo",
    "dev": "next dev",
    "build": "NODE_OPTIONS='--max-old-space-size=12288' next build",
    "relay": "yarn run relay:compile-default && yarn run relay:compile-dev",
    "sync": "npx cap sync",
    "run:ios": "yarn sync && npx cap run ios",
    "run:android": "yarn sync && npx cap run android"
  },
  "dependencies": {
    "@capacitor/android": "7.0.1",
    "@capacitor/camera": "latest",
    "@capacitor/core": "latest",
    "@capacitor/ios": "7.0.1",
    "@capacitor/splash-screen": "latest",
    "@next/bundle-analyzer": "15.1.7",
    "@skydio/nomad": "workspace:^",
    "@skydio/shared_ui": "workspace:^",
    "next": "14.2.3",
    "next-runtime-env": "3.2.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-relay": "16.1.0",
    "relay-runtime": "16.1.0",
    "tailwind-merge": "2.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.7",
    "@capacitor/cli": "latest",
    "copy-webpack-plugin": "12.0.2",
    "tailwindcss": "3.4.13",
    "ts-loader": "^6.1.0",
    "tslib": "2.6.3",
    "typescript": "5.7.2",
    "typescript-plugin-css-modules": "^2.3.0",
    "webpack": "5.74.0"
  },
  "author": "",
  "license": "ISC"
}
