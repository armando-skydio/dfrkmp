<?xml version="1.0" encoding="utf-8"?>
<font-family
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:targetApi="o"
    >
    <font
        android:font="@font/case_regular_normal"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/case_regular_normal"
        app:fontStyle="normal"
        app:fontWeight="400"
        />
    <font
        android:font="@font/case_regular_italic"
        android:fontStyle="italic"
        android:fontWeight="400"
        app:font="@font/case_regular_italic"
        app:fontStyle="italic"
        app:fontWeight="400"
        />
    <font
        android:font="@font/case_semibold_normal"
        android:fontStyle="normal"
        android:fontWeight="600"
        app:font="@font/case_semibold_normal"
        app:fontStyle="normal"
        app:fontWeight="600"
        />
    <font
        android:font="@font/case_semibold_italic"
        android:fontStyle="italic"
        android:fontWeight="600"
        app:font="@font/case_semibold_italic"
        app:fontStyle="italic"
        app:fontWeight="600"
        />
</font-family>
